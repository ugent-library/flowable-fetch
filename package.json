{
  "name": "flowable-fetch",
  "version": "2.4.1",
  "description": "Custom Node.js fetch method for the Flowable BPMN REST API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:ugent-library/flowable-fetch.git",
  "author": "Koen Verheyen <koen.verheyen@inuits.eu>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint . && tsc",
    "lint:bundled": "tsc dist/index.d.ts dist/index.d.mts",
    "non-dirty-check": "git diff --quiet --exit-code",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "precommit": [
    "lint",
    "build",
    "lint:bundled",
    "non-dirty-check"
  ],
  "dependencies": {
    "dotenv": "^16.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@fastify/pre-commit": "^2.0.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.45.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  }
}
